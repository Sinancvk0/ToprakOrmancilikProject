@model  CombinedModel
<section class="w3l-grids1">
    <div class="hny-three-grids py-5">
        <div class="container py-lg-5">
            <div class="row">
                @foreach (var item in Model.Documents)
                {
                    <div class="col-md-4 col-sm-6 mt-0 grids5-info">
                        <a href="#url" style="display: inline-block; width: 300px; height: 200px;">
                            <img src="@item.Image" class="img-fluid" style="width: 100%; height: 100%;" alt="">
                        </a>

                        <h4><a href="#url">@item.Title</a></h4>
                        <p>@item.Description</p>
                    </div>
                }



            </div>
        </div>
    </div>
</section>